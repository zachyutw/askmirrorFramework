{
    "name": "askmirror",
    "version": "1.0.0",
    "description": "This is  a MERN Framework with whole ES6 JS support.",
    "main": "server.js",
    "author": "zachyu.tw@gmail.com",
    "license": "MIT",
    "server": {
        "domain": "dev.askmirror.local"
    },
    "engines": {
        "node": "10.15.3",
        "npm": "6.4.1"
    },
    "scripts": {
        "start": "NODE_ENV=prod nodemon --exec babel-node src/server.js",
        "dev": "NODE_ENV=dev nodemon --exec babel-node src/server.js",
        "build": "NODE_ENV=prod babel src --out-dir dist",
        "serve": "NODE_ENV=prod node dist/server.js",
        "test": " NODE_ENV=test jest",
        "shcert": "rootdir=$(pwd) && ./scripts/cert/cert.script.sh ${rootdir} $npm_package_server_domain zachyu.tw@gmail.com"
    },
    "dependencies": {
        "@babel/runtime": "^7.4.2",
        "axios": "^0.18.0",
        "babel-polyfill": "^6.26.0",
        "body-parser": "^1.18.3",
        "boom": "^7.3.0",
        "compression": "^1.7.3",
        "cookie-session": "^1.3.3",
        "cors": "^2.8.5",
        "dotenv": "^7.0.0",
        "express": "^4.16.4",
        "express-session": "^1.15.6",
        "express-static-gzip": "^1.1.3",
        "faker": "^4.1.0",
        "helmet": "^3.16.0",
        "jsonwebtoken": "^8.5.1",
        "lodash": "^4.17.11",
        "moment": "^2.24.0",
        "mongoose": "^5.4.19",
        "mongoose-autopopulate": "^0.9.1",
        "morgan": "^1.9.1",
        "multer": "^1.4.1",
        "passport": "^0.4.0",
        "passport-custom": "^1.0.5",
        "passport-jwt": "^4.0.0",
        "passport-local": "^1.0.0",
        "passport-openid": "^0.4.0",
        "pug": "^2.0.3",
        "socket.io": "^2.2.0",
        "spdy": "^4.0.0",
        "uuid": "^3.3.2",
        "winston": "^3.2.1",
        "ws": "^6.2.0"
    },
    "devDependencies": {
        "@babel/cli": "^7.2.3",
        "@babel/core": "^7.3.4",
        "@babel/node": "^7.2.2",
        "@babel/plugin-transform-runtime": "^7.4.0",
        "@babel/preset-env": "^7.3.4",
        "babel-jest": "^24.5.0",
        "nodemon": "^1.18.10",
        "superagent": "^4.1.0",
        "supertest": "^4.0.2"
    },
    "jest": {
        "rootDir": "src",
        "setupFilesAfterEnv": [
            "./tests/jest.config.js"
        ],
        "testEnvironment": "node",
        "browser": false,
        "verbose": true
    }
